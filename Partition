use the asia table in praba database:
database_name:praba
____________________________________________________________________________________________________
select * from
(select *, rank() over(partition by Country order by Year) rnk from asia)a
where a.rnk=1;
________________________________________________________________________________________________

select * from
(select *, rank() over(partition by Country, Year order by Year) rnk from asia)a
where a.rnk=1;

________________________________________________________________________________________________
select *, first_value(Year) over(order by population)the_year
from asia;


________________________________________________________________________________________________

select *, last_value(Year) over(order by population desc rows between unbounded preceding and unbounded following)the_year
from asia;


________________________________________________________________________________________________

select *, round(first_value(Unemployment) over(order by population)- unemployment,2) no_work
from asia;


________________________________________________________________________________________________

select *, first_value(Unemployment) over(order by population) no_work,
round(first_value(Unemployment) over(order by population)-Unemployment,2) no_work_differ
from asia;



